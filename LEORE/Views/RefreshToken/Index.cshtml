@model IEnumerable<LEORE.Models.RefreshToken>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Token)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExpiresOn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RevokedOn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Token)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExpiresOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RevokedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Email)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RefreshTokensID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RefreshTokensID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RefreshTokensID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
